{
    "name": "panelomega/panel",
    "type": "project",
    "description": "The web application for the panel omega.",
    "keywords": [
        "panel",
        "panel omega",
        "web application"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "calebporzio/sushi": "^2.5",
        "codewithdennis/filament-simple-alert": "^3.0",
        "filament/filament": "^3.2",
        "intervention/image-laravel": "^1.3",
        "jaocero/radio-deck": "^1.2",
        "jelix/inifile": "^3.4",
        "jibaymcs/filament-tour": "^3.0",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "leandrocfe/filament-apex-charts": "^3.1",
        "marvinosswald/filament-input-select-affix": "^0.2.0",
        "monarobase/country-list": "^3.5",
        "outerweb/filament-settings": "^1.2",
        "stechstudio/filament-impersonate": "^3.8"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.16",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "pint": [
            "./vendor/bin/pint"
        ],
        "test": [
            "sudo omega-php artisan test"
        ],
        "test-coverage": [
            "Composer\\Config::disableProcessTimeout",
            "sudo omega-php artisan test --coverage -c \"phpunit-coverage.xml\""
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
